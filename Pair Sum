def pairSum(arr:list,n,target):
    result = []
    count = 0
    for i in range(len(arr)):
        for j in range(i + 1, len(arr)):
            if arr[i] + arr[j] == target:
                result.append((arr[i], arr[j]))
                count += 1
    if count == 0:
        return -1
    return count
